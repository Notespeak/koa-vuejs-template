{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "runner": "terminal",
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "build",
      "args": [
        "run",
        "build"
      ],
      "isBuildCommand": true
    },
    {
      "taskName": "start",
      "args": [
        "start"
      ]
    },
    {
      "taskName": "test",
      "args": [
        "test"
      ],
      "isTestCommand": true
    },
    {
      "taskName": "prestart",
      "args": [
        "run",
        "prestart"
      ]
    },
    {
      "taskName": "test:integration",
      "args": [
        "run",
        "test:integration"
      ]
    },
    {
      "taskName": "test:unit",
      "args": [
        "run",
        "test:unit"
      ]
    }
  ]
}
