{
  "name": "koa.vuejs.template",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test:api": "mocha --require ignore-styles --recursive api/build/api.test",
    "pretest:client": "./node_modules/typescript/bin/tsc -p ./web/",
    "test:client": "mocha --require ignore-styles --recursive web/build/web.test",
    "test": "npm run test:api && npm run test:web",
    "migrate": "ts-node ./node_modules/.bin/typeorm migrations:run",
    "create_migration": "ts-node ./node_modules/.bin/typeorm migrations:create -n",
    "prestart": "docker-compose up -d && webpack --config ./web/webpack.config.js",
    "start": "cd ./api/ && ../node_modules/.bin/nodemon -e ts --exec ../node_modules/.bin/ts-node ./server.ts",
    "start:prod": "NODE_ENV=production node ./app ./node_modules/.bin/nodemon -e ts --exec ./node_modules/.bin/ts-node --project ./api/ ./api/server.ts",
    "provision:prod": "ansible-playbook -l production -i ./ops/hosts ./ops/provision.yml",
    "prerelease": "webpack --config ./web/webpack.config.js",
    "deploy:prod": "npm run prerelease && ansible-playbook -l production -i ./ops/hosts ./ops/deploy.yml",
    "ssh:prod": "ssh `grep 'deploy_user=' ./ops/hosts | tail -n1 | awk -F'=' '{ print $2}'`@`awk 'f{print;f=0} /[production]/{f=1}' ./ops/hosts | head -n 1`"
  },
  "author": "Brady Holt",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/chai": "^3.4.32",
    "@types/koa": "^2.0.39",
    "@types/mocha": "^2.2.31",
    "@types/node": "^7.0.22",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.0",
    "extendify": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.22.0",
    "jsonwebtoken": "^7.4.1",
    "koa": "^2.2.0",
    "koa-body-parser": "^1.1.2",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.0",
    "koa-static": "^3.0.0",
    "massive": "^2.6.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.4",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.0-alpha.14",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.4",
    "typeorm": "^0.1.0-alpha.3",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "vue": "^2.2.6",
    "vue-class-component": "^5.0.1",
    "vue-loader": "^11.3.4",
    "vue-property-decorator": "^5.0.1",
    "vue-router": "^2.3.1",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.3.3",
    "webpack-hot-middleware": "^2.18.0",
    "whatwg-fetch": "^2.0.3"
  }
}
